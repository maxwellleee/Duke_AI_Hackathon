# Copy this file to .env (or .env.local) and fill in values from Firebase Console -> Project settings -> Your apps
# Vite requires env vars prefixed with VITE_

VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here

# Optional: backend URL used by the frontend hook to verify tokens
VITE_API_URL=http://localhost:8000
